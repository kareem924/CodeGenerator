<div class="modal fade" id="updateConfirmModal" tabindex="-1" role="dialog" aria-labelledby="deleteLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="deleteLabel">@(global::Resources.Resource.UnAssignReference)</h4>
            </div>
            @using (Html.BeginForm("Unassign", "GeneratedReference", FormMethod.Post, new { @class = "form-horizontal", id = "UpdateForm" }))
            {

                @Html.ValidationSummary(true)
                <div class="modal-body" id="updatePreview">
                    <p>Loading content...</p>



                </div>


                <div class="modal-footer">
                    <button type="button" class="btn btn-success" data-dismiss="modal">@(global::Resources.Resource.Cancel)</button>
                    <button type="submit" class="btn btn-danger">@(global::Resources.Resource.UnAssignGeneratedReference)</button>
                </div>
            }
        </div>
    </div>
</div>

