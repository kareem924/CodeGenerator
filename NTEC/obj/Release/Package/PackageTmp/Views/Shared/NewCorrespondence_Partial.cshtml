<script type="text/javascript">
    function inputNumOnly(e, obj) {
        // only allows inputting of valid number character
        var keynum;

        // catch all special keyws and allow them
        v = e.keyCode

        if (v == 8 || v == 9 || v == 13 || v == 46)
            return true;



        if (window.event) // IE
        {
            keynum = String.fromCharCode(e.keyCode);
        }
        else if (e.which) // Netscape/Firefox/Opera
        {
            keynum = String.fromCharCode(e.which);
        }


        if (keynum == '0' || keynum == '1' || keynum == '2' || keynum == '3' || keynum == '4' || keynum == '5' || keynum == '6' || keynum == '7' || keynum == '8' || keynum == '9' || keynum == '.')
            return true;
        return false;
    }

</script>
