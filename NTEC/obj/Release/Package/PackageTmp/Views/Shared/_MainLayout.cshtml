@using NTEC.Classes.Security


<!DOCTYPE html>
<html>
<head>
    @*<meta charset="utf-8" />
        <meta name="viewport" content="width=device-width" />*@
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="shortcut icon" href="@Url.Content("~/favicon.ico")">
    <link rel="stylesheet" id="arabic-font-css" href="http://fonts.googleapis.com/earlyaccess/droidarabickufi.css?ver=3.9.1" type="text/css" media="all">
    <script type="text/javascript">
        window.ApplicationBasePath = '@Url.Content("~")';
        window.appLanguageId = '@SessionManager.LanguageId';
        window.gridUserSetting = '@SessionManager.GetGridSetting';


    </script>



    @*@Styles.Render("~/Content/css")*@
    @Styles.Render("~/Content/themes/base/css")


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/jqueryval")
    @*@Scripts.Render("~/bundles/appcore")*@
    @Scripts.Render("~/bundles/common")
    @Scripts.Render("~/bundles/OthersCommen")

    <script src="~/Scripts/app/core/app.js"></script>
    <script src="~/Scripts/app/core/app.globals.js"></script>
    <script src="~/Scripts/app/core/app.debugger.js"></script>
    <script src="~/Scripts/app/core/app.resources.js"></script>
    <script src="~/Scripts/app/core/app.ui.common.js"></script>
    <script src="~/Scripts/app/core/app.ui.enums.js"></script>
    <script src="~/Scripts/app/core/app.ui.js"></script>

    <link href="~/Content/themes/redmond/jquery-ui-1.10.4.custom.css" rel="stylesheet" />


    <script src="~/Scripts/lib/ckeditor/ckeditor.js"></script>
    <script src="~/Scripts/lib/ckeditor/adapters/jquery.js"></script>

    @{
        if (@SessionManager.LanguageId == "1")
        {
            // css and js files regarding the selected language
            <link href="~/Content/css/ar/PineNotifications/jquery.pnotify.default.css" rel="stylesheet" />
            <script src="~/Content/css/ar/bootstrap.min.js"></script>
            <link href="~/Content/css/ar/bootstrap.min.css" rel="stylesheet" />
            <link href="~/Content/css/ar/login-ar.css" rel="stylesheet" />
            <link href="~/Content/css/ar/corr-ar.css" rel="stylesheet" />
            <link href="~/Content/css/ar/ui.dynatree.css" rel="stylesheet" />

        }
        else
        {
            <link href="@Url.Content("~/Content/PineNotifications/jquery.pnotify.default.css")" rel="stylesheet" type="text/css" />
            <link href=@Url.Content("~/content/css/bootstrap.min.css") rel="stylesheet">
            <script src="~/Scripts/lib/bootstrap.min.js"></script>

            <link href="~/Content/css/login-en.css" rel="stylesheet" />
            <link href="~/Content/css/corr-en.css" rel="stylesheet" />
            <link href="~/Scripts/lib/dynaTree/skin/ui.dynatree.css" rel="stylesheet" />
        }
    }
    <link href="~/Content/Site.css" rel="stylesheet" />
    <link href="~/Content/css/ui.jqgrid.css" rel="stylesheet" />
    <script src="~/Scripts/lib/jQGrid/i18n/grid.locale-en.js"></script>
    @*<script src="~/Scripts/lib/jQGrid/jquery.jqGrid.min.js"></script>*@

    <script src="~/Scripts/lib/jQGrid/jquery.jqGrid.src.js"></script>
    <script src="~/Scripts/lib/bootbox.min.js"></script>



    <link href="~/Content/FineUploader/fineuploader.css" rel="stylesheet" />
    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet" />


    <script src="~/Scripts/lib/momentjs/moment.min.js"></script>
    <script src="~/Scripts/lib/momentjs/moment-with-locales.min.js"></script>


    <script src="~/Scripts/lib/dynaTree/jquery.dynatree.min.js"></script>

    @RenderSection("Header", required: false)
</head>
<body>

    <script type="text/javascript">
        // to get all resources translations for using inside js files

        SampleNS.Resources.Welcome = '@(global::Resources.Resource.Welcome)';
     
        

    </script>
    @*// Ajax Call animation*@
    <div id="divProgressOverlay" class="UpdateProgressOverlay">

        <div class="spinnerr">
            <div class="cube1"></div>
            <div class="cube2"></div>
        </div>
    </div>


    @RenderBody()

    

</body>
</html>
